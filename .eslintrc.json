{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "consistent-return": "off",
    "func-names": "off",
    "no-console": "off",
    "no-process-exit": "off",
    "node/no-unpublished-require": "off",
    "no-multi-assign": "off"
  }
}
